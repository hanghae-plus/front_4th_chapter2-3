{
  "name": "front-end-2-3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "api:init": "mkdir -p quicktype",
    "api:fetch:posts": "npm run api:init && curl \"https://dummyjson.com/posts?limit=10&skip=0\" > quicktype/postsResponse.json",
    "api:fetch:users": "npm run api:init && curl \"https://dummyjson.com/users\" > quicktype/usersResponse.json",
    "api:fetch:comments": "npm run api:init && curl \"https://dummyjson.com/comments/post/1\" > quicktype/commentsResponse.json",
    "api:fetch:tags": "npm run api:init && curl \"https://dummyjson.com/posts/tags\" > quicktype/tagsResponse.json",
    "api:fetch": "npm run api:fetch:posts && npm run api:fetch:users && npm run api:fetch:comments && npm run api:fetch:tags",
    "api:types:posts": "npx quicktype quicktype/postsResponse.json -o quicktype/postsTypes.ts",
    "api:types:users": "npx quicktype quicktype/usersResponse.json -o quicktype/usersTypes.ts",
    "api:types:comments": "npx quicktype quicktype/commentsResponse.json -o quicktype/commentsTypes.ts",
    "api:types:tags": "npx quicktype quicktype/tagsResponse.json -o quicktype/tagsTypes.ts",
    "api:types:generate": "npm run api:types:posts && npm run api:types:users && npm run api:types:comments && npm run api:types:tags",
    "api:types": "npm run api:fetch && npm run api:types:generate"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.64.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-select": "^2.1.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.0",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "lucide-react": "^0.473.0",
    "msw": "^2.7.0",
    "prettier": "^3.4.2",
    "quicktype": "^23.0.171",
    "react-router-dom": "^7.1.3",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.7",
    "vitest": "^3.0.2",
    "vitest-browser-react": "^0.0.4"
  }
}
